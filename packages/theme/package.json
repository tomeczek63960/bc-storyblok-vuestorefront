{
  "name": "@vue-storefront/bigcommerce-theme",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "nuxt build -m",
    "build:analyze": "nuxt build -a -m",
    "dev": "nuxt",
    "generate": "nuxt generate",
    "lint": "eslint . --ext .ts,.vue",
    "lint:fix": "eslint . --ext .ts,.vue --fix",
    "precommit": "lint-staged",
    "start": "nuxt start",
    "test": "jest",
    "test:e2e": "cypress open --config-file tests/e2e/cypress.json",
    "test:e2e:generate:report": "yarn -s mochawesome-merge \"tests/e2e/report/*.json\" > \"tests/e2e/report.json\" && yarn -s marge tests/e2e/report.json -o \"tests/e2e/report\"",
    "test:e2e:hl": "cypress run --headless --config-file tests/e2e/cypress.json",
    "update:check": "ncu",
    "update:update": "ncu -u"
  },
  "dependencies": {
    "@bigcommerce/checkout-sdk": "^1.212.0",
    "@nuxtjs/composition-api": "^0.31.0",
    "@nuxtjs/google-fonts": "^1.3.0",
    "@nuxtjs/markdownit": "^2.0.0",
    "@nuxtjs/pwa": "^3.3.5",
    "@nuxtjs/style-resources": "^1.2.1",
    "@storefront-ui/vue": "0.12.0",
    "@vue-storefront/bigcommerce": "^1.0.0",
    "@vue-storefront/bigcommerce-api": "^1.0.0",
    "@vue-storefront/middleware": "~2.5.4",
    "@vue-storefront/nuxt": "~2.5.4",
    "@vue-storefront/nuxt-theme": "~2.5.4",
    "@vue-storefront/storyblok": "^0.3.0",
    "cookie-universal-nuxt": "^2.1.5",
    "core-js": "^3.21.0",
    "dotenv": "^15.0.0",
    "nuxt": "^2.15.8",
    "nuxt-i18n": "^6.5.0",
    "storyblok-vue": "^1.0.5",
    "vee-validate": "^3.4.14",
    "vue-lazy-hydration": "~2.0.0-beta.4",
    "vue-scrollto": "^2.20.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.11",
    "@nuxt/types": "^2.15.8",
    "@types/jest": "^27.4.0",
    "@vue/test-utils": "^1.3.0",
    "babel-jest": "^27.4.6",
    "cypress": "^9.4.1",
    "cypress-pipe": "^2.0.0",
    "cypress-tags": "^0.3.0",
    "jest": "^27.4.7",
    "mochawesome": "^7.0.1",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.0.1",
    "vue-jest": "3.0.7"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
